{
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "https://storage.reviewniverse.net/sitemap/sitemap.xml"
    },
    {
      "source": "/sitemap/contents-1.xml",
      "destination": "https://storage.reviewniverse.net/sitemap/contents-1.xml"
    },
    {
      "source": "/sitemap/collections-1.xml",
      "destination": "https://storage.reviewniverse.net/sitemap/collections-1.xml"
    },
    {
      "source": "/sitemap/genres-1.xml",
      "destination": "https://storage.reviewniverse.net/sitemap/genres-1.xml"
    },
    {
      "source": "/sitemap/people-1.xml",
      "destination": "https://storage.reviewniverse.net/sitemap/people-1.xml"
    },
    {
      "source": "/sitemap/productions-1.xml",
      "destination": "https://storage.reviewniverse.net/sitemap/productions-1.xml"
    },
    {
      "source": "/__/auth/:path*",
      "destination": "https://reviewniverse-899d2.firebaseapp.com/__/auth/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "sameorigin"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/users/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=600, stale-while-revalidate=59"
        }
      ]
    }
  ],
  "trailingSlash": false,
  "cleanUrls": true
}
